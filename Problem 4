#include<iostream>
#include<conio.h>
using namespace std;

int main ()

{
	int first, second, third;
	
	cout<< "Enter first number: "; cin>> first;
	cout<< "Enter second number: "; cin>> second;
	cout<< "Enter third number: "; cin>> third;
	
	if (first >= second && first >= third)
		cout<< "The largest among the three numbers is: " << first;
	else if (second >= first && second >= third)
		cout<< "The largest among the three numbers is: " << second;
	else
		cout<< "The largest among the three numbers is: " << third;
		
	
	_getch();
	return 0;
}
