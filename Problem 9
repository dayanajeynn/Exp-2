#include<iostream>
#include<conio.h>	

using namespace std;

	
	int main()
	{
		int rows, columns, i, j;
		
		cout<< "Enter the number of rows: ";
		cin>> rows;
		
		cout<< "Enter the number of columns: ";
		cin>> columns;
		
		for(j=1;j<=rows;j++)
		{
			for (i=1; i<=columns; i++)
			{
				cout<< "*";
			}
			cout<<endl;
		}
	
	
	_getch();
	return 0;
	
}
