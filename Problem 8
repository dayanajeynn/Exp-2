#include<iostream>
#include<conio.h>

using namespace std;

int main ()
{
	
	int number, n1, n2, n3, n4, n5, n6, n7, n8, n9, n10;
	
	cout<< "Enter a number: "; cin>> number;
	cout<< "Multiplication table of " << number <<":" <<endl;
	
	n1 = 1 * number;
	n2 = 2 * number;
	n3 = 3 * number;
	n4 = 4 * number;
	n5 = 5 * number;
	n6 = 6 * number;
	n7 = 7 * number;
	n8 = 8 * number;
	n9 = 9 * number;
	n10 = 10 * number;
	
	cout<< "1 * " << number  << " = " << n1 <<endl;
	cout<< "2 * " << number  << " = " << n2 <<endl;
	cout<< "3 * " << number  << " = " << n3 <<endl;
	cout<< "4 * " << number  << " = " << n4 <<endl;
	cout<< "5 * " << number  << " = " << n5 <<endl;
	cout<< "6 * " << number  << " = " << n6 <<endl;
	cout<< "7 * " << number  << " = " << n7 <<endl;
	cout<< "8 * " << number  << " = " << n8 <<endl;
	cout<< "9 * " << number  << " = " << n9 <<endl;
	cout<< "10 * " << number  << " = " << n10 <<endl;

	
	
	
	_getch();
	return 0;
	
}
